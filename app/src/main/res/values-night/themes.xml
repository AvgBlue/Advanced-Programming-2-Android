<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="DarkTheme" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
    </style>

    <style name="background" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <item name="android:background">@drawable/background_portrait_dark</item>
    </style>

    <style name="textColor" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="primaryButton" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="android:backgroundTint">@color/light_gray_green</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="secondaryButton" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="android:backgroundTint">@color/dark_gray_green</item>
        <item name="android:textColor">@color/white</item>
    </style>

</resources>